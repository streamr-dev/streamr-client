{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "loose": false,
        "targets": {
          "browsers": [
            "last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-modules-commonjs",
    ["@babel/plugin-proposal-class-properties",
      { "loose": false }
    ]
  ]
}
